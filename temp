int c;
int a[5];
int insertion_sort(int n)
{
    int i,j,k;
    i=1;
    while(i<n){ 
        j=i;
        while(j>0) {
            if(a[j]<a[j-1]) 
            {
                k=a[j-1];
                a[j-1]=a[j];
                a[j]=k;
            }
            j--;
        }
        i++;
    }
    return 1;
}
int main()
{
    
    a[0]=3;
    a[1]=2;
    a[2]=4;
    a[3]=1;
    a[4]=2;
    insertion_sort(5);
}